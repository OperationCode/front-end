{
  "name": "operation-code",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "build-storybook": "build-storybook -s ./static -c .storybook -o .storybook-dist",
    "build": "next build",
    "dev": "next",
    "export": "next export",
    "format-all": "yarn format:scripts && yarn format:styles",
    "format:scripts": "prettier --write \"**/*.js\" && eslint --fix \"**/*.js\"",
    "format:styles": "prettier --write \"**/*.css\" && stylelint --fix \"**/*.css\"",
    "lint:scripts": "eslint \"**/*.js\"",
    "lint:styles": "stylelint \"**/*.css\"",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "now-build": "echo 'Built on Travis CI'",
    "precommit": "lint-staged && yarn test",
    "start": "next start",
    "storybook": "start-storybook -s ./static -p 9001 -c .storybook",
    "test:ci": "NODE_ENV=test BABEL_ENV=test jest --ci --coverage --coverageReporters=text-lcov | yarn coveralls",
    "test:update-snaps": "jest -u",
    "test": "NODE_ENV=test BABEL_ENV=test jest"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "eslint --fix"],
    "*.css": ["prettier --write", "stylelint --fix"]
  },
  "dependencies": {
    "@babel/core": "7.0.0",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-brands": "5.0.13",
    "@fortawesome/fontawesome-free-regular": "5.0.13",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "@storybook/addon-a11y": "^3.4.10",
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-backgrounds": "^3.4.10",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-knobs": "^3.4.10",
    "@storybook/addon-options": "^3.4.10",
    "@storybook/addon-storyshots": "^3.4.10",
    "@storybook/addon-viewport": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@zeit/next-css": "^0.2.0",
    "autoprefixer": "^9.1.3",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "next": "^6.1.1",
    "path": "^0.12.7",
    "postcss-export-custom-variables": "^1.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-ga": "^2.5.3",
    "react-modal": "^3.5.1",
    "react-scroll": "^1.7.10",
    "react-youtube": "^7.6.0",
    "style-loader": "^0.23.0",
    "universal-cookie": "^3.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "coveralls": "^3.0.2",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "jest-dom": "^1.12.0",
    "jest-enzyme": "^6.0.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "react-testing-library": "^5.0.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
