    "@storybook/addons": "6.0.28",
    "@storybook/react": "6.1.15",
    "@storybook/theming": "^6.1.15",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "autoprefixer": "9.8.6",
    "axios-mock-adapter": "^1.19.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
