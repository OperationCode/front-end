    "logrocket-react": "^4.0.1",
    "next": "11.1.2",
    "next-cookies": "^2.0.3",
    "nuka-carousel": "^4.8.0",
    "object-hash": "^2.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-modal": "^3.14.3",
    "react-on-screen": "^2.1.1",
    "react-player": "^2.9.0",
    "react-scroll": "^1.8.4",
    "react-scroll-up-button": "^1.6.4",
    "react-select": "^4.0.2",
    "react-youtube": "7.13.1",
    "yup": "^0.32.9"
