{
  "name": "operationcode",
  "license": "MIT",
  "version": "2.0.15",
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=1 next build",
    "create-component": "node scripts/createComponent/createComponent.js",
    "create-page": "node scripts/createPage/createPage.js",
    "dev": "next",
    "format": "prettier --write \"**/*\"",
    "format:md": "prettier --write \"**/*.md\"",
    "lint": "yarn lint:scripts && yarn lint:styles && yarn lint:md && yarn format:md",
    "lint:ci": "yarn eslint \"**/*.js\" && yarn stylelint \"**/*.css\"",
    "lint:md": "eslint --fix \"**/*.mdx\" && remark .",
    "lint:scripts": "eslint --fix \"**/*.js\"",
    "lint:styles": "stylelint --fix \"**/*.css\"",
    "start": "next start",
    "storybook": "start-storybook -s ./public -p 9001 -c .storybook",
    "storybook:build": "build-storybook -s ./public -c .storybook -o .storybook-dist",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:update-snaps": "cypress run --env updateSnapshots=true",
    "test:e2e:ci": "cypress run --record",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest --maxWorkers=2",
    "test:ci": "yarn test --ci --coverage=true --verbose=false --silent=true",
    "test:changes": "yarn test --changedSince=main --verbose=true",
    "test:changes:watch": "yarn test:changes --watch",
    "test:watch": "yarn test --watch",
    "test:update-snaps": "yarn test -u"
  },
  "nyc": {
    "extends": "./nyc.config.js",
    "report-dir": "cypress-coverage",
    "reporter": [
      "json",
      "lcov"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-lint-are-links-valid"
    ]
  },
  "dependencies": {
    "@innocuous/components": "^2.1.1",
    "@innocuous/functions": "^2.1.1",
    "@next/bundle-analyzer": "^10.0.6",
    "@sentry/browser": "^5.28.0",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "fast-xml-parser": "^3.17.6",
    "fingerprintjs2": "^2.1.2",
    "fontfaceobserver": "^2.1.0",
    "formik": "^2.2.6",
    "history": "^5.0.0",
    "intersection-observer": "^0.12.0",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "logrocket": "^1.0.14",
    "logrocket-react": "^4.0.1",
    "next": "^10.0.6",
    "next-cookies": "^2.0.3",
    "nuka-carousel": "^4.7.5",
    "object-hash": "^2.1.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "qs": "^6.9.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-modal": "^3.12.1",
    "react-on-screen": "^2.1.1",
    "react-player": "^2.8.2",
    "react-scroll": "^1.8.1",
    "react-scroll-up-button": "^1.6.4",
    "react-select": "^4.0.2",
    "react-youtube": "7.13.1",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@cypress/code-coverage": "3.9.2",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.6",
    "@operation_code/eslint-plugin-custom-rules": "^1.0.1",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-essentials": "6.0.28",
