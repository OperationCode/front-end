{
  "name": "operationcode",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 next build",
    "build-storybook": "build-storybook -s ./static -c .storybook -o .storybook-dist",
    "build": "next build",
    "create-component": "node scripts/createComponent/createComponent.js",
    "create-page": "node scripts/createPage/createPage.js",
    "dev": "node server.js",
    "export": "next export",
    "format": "yarn format:scripts && yarn format:styles",
    "format:scripts": "prettier --write \"**/*.js\" && eslint --fix \"**/*.js\"",
    "format:styles": "prettier --write \"**/*.css\" && stylelint --fix \"**/*.css\"",
    "lint": "yarn lint:scripts && yarn lint:styles",
    "lint:scripts": "eslint \"**/*.js\"",
    "lint:styles": "stylelint \"**/*.css\"",
    "now-build": "echo 'Built on Travis CI'",
    "precommit": "lint-staged && yarn test:unit:changes",
    "start": "NODE_ENV=production node server.js",
    "storybook": "start-storybook -s ./static -p 9001 -c .storybook",
    "test:e2e": "cypress open",
    "test:e2e:ci": "cypress run --record --key 9e628491-4a66-4f7e-9651-91ab6ac459b8",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "test:unit:ci": "yarn test --ci --coverage",
    "test:unit:changes": "yarn test --changedSince master",
    "test:unit:changes:watch": "yarn test --changedSince master --watch",
    "test:unit:watch": "yarn test --watch",
    "test:update-snaps": "yarn test -u"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-a11y": "3.4.11",
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-backgrounds": "3.4.11",
    "@storybook/addon-info": "3.4.11",
    "@storybook/addon-knobs": "3.4.11",
    "@storybook/addon-options": "3.4.11",
    "@storybook/addon-storyshots": "3.4.11",
    "@storybook/addon-viewport": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react": "3.4.11",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.5.0",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "fontfaceobserver": "^2.1.0",
    "formik": "^1.5.1",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "next": "^8.0.4",
    "next-redux-wrapper": "^2.1.0",
    "nuka-carousel": "^4.5.3",
    "path": "^0.12.7",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^8.0.9",
    "postcss-export-custom-variables": "^1.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-prepend-imports": "^1.0.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-cookie": "^3.1.2",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.6",
    "react-modal": "^3.8.1",
    "react-on-screen": "^2.1.1",
    "react-redux": "^6.0.0",
    "react-scroll": "^1.7.10",
    "react-scroll-up-button": "^1.6.4",
    "react-select": "^2.4.2",
    "react-svg-loader": "^2.1.0",
    "react-youtube": "^7.8.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.2",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.6.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.6.0",
    "jest-enzyme": "7.0.2",
    "jest-prop-type-error": "^1.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.15.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "start-server-and-test": "^1.7.13",
