{
  "name": "operationcode",
  "license": "MIT",
  "version": "1.3.2",
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=1 next build",
    "now-build": "next build",
    "create-component": "node scripts/createComponent/createComponent.js",
    "create-page": "node scripts/createPage/createPage.js",
    "dev": "next",
    "format:md": "prettier --write \"**/*.md\"",
    "lint": "yarn lint:scripts && yarn lint:styles && yarn format:md",
    "lint:scripts": "eslint --fix \"**/*.js\"",
    "lint:styles": "stylelint --fix \"**/*.css\"",
    "precommit": "lint-staged && yarn test:changes",
    "start": "next start",
    "storybook": "start-storybook -s ./static -p 9001 -c .storybook",
    "storybook:build": "build-storybook -s ./static -c .storybook -o .storybook-dist",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:update-snaps": "cypress run --env updateSnapshots=true",
    "test:e2e:ci": "cypress run --record",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "test:ci": "yarn test --ci --coverage",
    "test:changes": "yarn test --changedSince master",
    "test:changes:watch": "yarn test --changedSince master --watch",
    "test:watch": "yarn test --watch",
    "test:update-snaps": "yarn test -u"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
