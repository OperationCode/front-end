    "intersection-observer": "^0.12.2",
    "lodash": "^4.17.21",
    "logrocket": "^10.1.0",
    "logrocket-react": "^6.0.3",
    "next": "^12.3.1",
    "next-cookies": "^2.0.3",
    "next-sitemap": "^4.2.3",
    "object-hash": "^3.0.0",
    "path": "^0.12.7",
